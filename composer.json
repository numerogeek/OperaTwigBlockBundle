{
    "name": "opera-project/twig-block-bundle",
    "description": "The `OperaTwigBlockBundle` provide a way to put template in blocks",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Lombardot",
            "email": "cedric.lombardot@gmail.com"
        }
    ],
    "require": {
        "opera-project/core-bundle": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Opera\\TwigBlockBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
